<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Fetch</servlet-name>
		<servlet-class>com.ashish.cdn.Fetch</servlet-class>
		<load-on-startup>-11</load-on-startup>
		<!-- intentionally putting as -11 to save appengine instances -->
	</servlet>
	<servlet-mapping>
		<servlet-name>Fetch</servlet-name>
		<url-pattern>/fetch/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Fetch2</servlet-name>
		<servlet-class>com.ashish.cdn.Fetch2</servlet-class>
		<load-on-startup>-11</load-on-startup>
		<!-- intentionally putting as -11 to save appengine instances -->
	</servlet>
	<servlet-mapping>
		<servlet-name>Fetch2</servlet-name>
		<url-pattern>/fetch2/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>query</servlet-name>
		<jsp-file>/WEB-INF/jsp/query.jsp</jsp-file>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>query</servlet-name>
		<url-pattern>/query/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>eweb</servlet-name>
		<jsp-file>/e-web/index.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>eweb</servlet-name>
		<url-pattern>/web/*</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>404</error-code>
		<location>/WEB-INF/jsp/404.jsp</location>
	</error-page>
</web-app>
